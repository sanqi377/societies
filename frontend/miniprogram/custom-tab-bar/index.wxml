<view class="tab-bar">
  <view class="content">
    <view wx:for="{{list}}" wx:key="index" class="tab-bar-item" data-path="{{item.pagePath}}" data-index="{{index}}" bindtap="switchTab">
      <image class="icon" src="{{selected === index ? item.selectedIconPath : item.iconPath}}" />
    </view>
  </view>
</view>